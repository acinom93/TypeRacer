<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="monash.sydney.edu.au.typeracing">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ourlogosmall"
        android:label=""
        android:roundIcon="@drawable/ourlogosmall"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SelectInterestActivity" />
        <activity android:name=".HomeScreen" />
        <activity android:name=".PlayGameActivity">

        </activity>
        <activity android:name=".ShowStat"></activity>
        <activity android:name=".AboutActivity"></activity>
        <activity android:name=".ContactUs" />
        <activity android:name=".CreaditActivity"></activity>

        <meta-data
            android:name="com.google.android.gms.games.APP_ID"
            android:value="@string/app_id" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>